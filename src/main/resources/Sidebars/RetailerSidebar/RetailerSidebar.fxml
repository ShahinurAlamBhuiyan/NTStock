<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="598.0" prefWidth="246.0" style="-fx-background-color: rgb(47,216,103);" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Sidebars.RetailerSidebar.RetailerSidebarController">
   <children>
      <VBox alignment="TOP_CENTER" prefHeight="598.0" prefWidth="246.0">
         <children>
            <VBox alignment="TOP_CENTER" prefHeight="514.0" prefWidth="246.0" spacing="13.0">
               <children>
                  <VBox prefHeight="29.0" prefWidth="246.0" />
                  <Button fx:id="MyProductsBtn" alignment="CENTER" mnemonicParsing="false" onAction="#handleMyProductsBtn" opacity="0.58" prefHeight="60.0" prefWidth="220.0" style="-fx-background-color: #142521;" styleClass="sidebar" text="My Products" textFill="#f5f5f5">
                     <font>
                        <Font name="Arial Bold" size="20.0" />
                     </font>
                     <cursor>
                        <Cursor fx:constant="HAND" />
                     </cursor>
                     <graphic>
                        <ImageView fitHeight="44.0" fitWidth="44.0" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@../../Image/icons/products.png" />
                           </image>
                        </ImageView>
                     </graphic>
                  </Button>
                  <Button fx:id="DealersListBtn" alignment="CENTER" mnemonicParsing="false" onAction="#handleDealersListBtn" opacity="0.58" prefHeight="60.0" prefWidth="220.0" style="-fx-background-color: #142521;" styleClass="sidebar" text="Dealers List" textFill="#f5f5f5">
                     <font>
                        <Font name="Arial Bold" size="20.0" />
                     </font>
                     <graphic>
                        <ImageView fitHeight="39.0" fitWidth="39.0" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@../../Image/icons/groups.png" />
                           </image>
                        </ImageView>
                     </graphic>
                     <cursor>
                        <Cursor fx:constant="HAND" />
                     </cursor>
                  </Button>
                  <Button fx:id="MyDealersListBtn" alignment="CENTER" mnemonicParsing="false" onAction="#handleMyDealersListBtn" opacity="0.58" prefHeight="60.0" prefWidth="220.0" style="-fx-background-color: #142521;" styleClass="sidebar" text="My Dealers" textFill="#f5f5f5">
                     <font>
                        <Font name="Arial Bold" size="20.0" />
                     </font>
                     <graphic>
                        <ImageView fitHeight="39.0" fitWidth="39.0" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@../../Image/icons/peoples.png" />
                           </image>
                        </ImageView>
                     </graphic>
                     <cursor>
                        <Cursor fx:constant="HAND" />
                     </cursor>
                  </Button>
               </children>
            </VBox>
            <Button alignment="CENTER" mnemonicParsing="false" onAction="#handleLogout" opacity="0.58" prefHeight="60.0" prefWidth="200.0" style="-fx-background-color: #142521;" styleClass="sidebar" text="   Log Out" textFill="#f5f5f5">
               <font>
                  <Font name="Arial Bold" size="25.0" />
               </font>
               <graphic>
                  <ImageView fitHeight="40.0" fitWidth="52.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../../Image/icons/logout.png" />
                     </image>
                  </ImageView>
               </graphic>
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor>
            </Button>
         </children>
      </VBox>
   </children>
